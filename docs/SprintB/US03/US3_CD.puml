@startuml
skinparam classAttributeIconSize 0

class Client {
  -int citizen card number
  -int NHS number
  -int birth date
  -String sex
  -int TIF
  -int phone number
  -String e-mail
  -String name
  +Client(name, citizencardnumber,\n nhs, date, sex, tin, phonenumber, email)
}

class Company {
  +getClientStore()
}

class ClientStore {
  +createClient(cldto)
  +validateClient(cl)
  +saveClient(cl)
  +addClient(cl)
}

class ClientDto {
  -int citizen card number
  -int NHS number
  -int birth date
  -String sex
  -int TIF
  -int phone number
  -String e-mail
  -String name
}

class ClientMapper {
+getname()
+getnhs()
+getdate()
+getsex()
+gettin()
+getphonenumber()
+getemail()
}


class CreateClientController {
  +createClient()
  +saveClient()
}

class AuthFacade {
  +addUser(name,email,pwd)
}

class CreateClientUI {

}

CreateClientUI ..> CreateClientController
CreateClientUI ..> ClientDto
CreateClientController ..> Company
CreateClientController ..> ClientDto
CreateClientController ..> ClientMapper
CreateClientController ..> Client
CreateClientController ..> ClientStore
ClientMapper ..> ClientDto
ClientMapper ..> Client
ClientStore "1" -> "*" Client : register the
ClientStore "1" -> "1" AuthFacade : uses
Company "1" -> "1" ClientStore : uses


@enduml