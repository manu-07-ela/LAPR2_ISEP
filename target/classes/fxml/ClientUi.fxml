<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox prefHeight="400.0" prefWidth="286.0" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="172.0" prefWidth="267.0" />
         </children>
      </VBox>
   </center>
   <left>
      <VBox prefHeight="400.0" prefWidth="239.0" style="-fx-background-color: #EAEEEE;" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="72.0" prefWidth="239.0">
               <children>
                  <Label alignment="CENTER" text="Welcome" textFill="#154d5f">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <Label alignment="CENTER" text="to your personal area" textFill="#239ba1">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="5.0" right="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <Separator prefWidth="200.0" />
            <HBox alignment="CENTER" prefHeight="43.0" prefWidth="229.0" style="-fx-background-color: #FFFFFF; -fx-background-radius: 30; -fx-effect: dropShadow(three-pass-box,rgba(0,0,0,0.1), 10.0 , 0.0 , 0.0 , 10.0);">
               <children>
                  <ImageView fitHeight="31.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/miscroscopio.png" />
                     </image>
                  </ImageView>
                  <TextField alignment="CENTER" prefHeight="39.0" prefWidth="123.0" promptText="What do you need?" style="-fx-background-color: rgba(0,0,0,0);">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </TextField>
                  <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="56.0" style="-fx-background-color: #239ba1; -fx-background-radius: 30;" text="Search " textFill="WHITE">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets left="5.0" right="5.0" top="20.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" prefHeight="41.0" prefWidth="239.0">
               <children>
                  <ImageView fitHeight="30.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/logout2.png" />
                     </image>
                  </ImageView>
               </children>
               <VBox.margin>
                  <Insets top="200.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </left>
   <right>
      <VBox prefHeight="400.0" prefWidth="29.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="26.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/closeGreen.png" />
               </image>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </ImageView>
         </children>
      </VBox>
   </right>
</BorderPane>
